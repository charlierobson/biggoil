 #!/bin/bash 
function compress {
    mono /usr/local/bin/Brass.exe $1.txt $1.bin
    ./lz48 -i $1.bin -o $1.binlz
    rm $1.bin
}

compress title
compress lvl1
compress lvl2
compress lvl3
compress lvl4

mono /usr/local/bin/Brass.exe main.asm biggoil.p -s -l biggoil.html

cp biggoil.p zxpandsdcard/menu.p
cp biggoil.p.sym zxpandsdcard/menu.p.sym
